<h1><%= @auction.name %></h1>

<% if @auction.listings.any? %>
<h2>Listings</h2>
<%= @auction.listings.each do |listing|%>
<%= link_to listing.title, listing_path(listing) %>
<% end%>
<% end %>

<p>Admin: <%= @auction.admin.name %></p>

<% if (current_user == @auction.admin) %>



    <%= link_to "Create New Listing", new_auction_listing_path(@auction)%>
<% end%>